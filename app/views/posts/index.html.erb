

<div class="row">
  <div id="blog-content" class="seven columns panel">
    <% @posts.each do |post| %>
      <h2><%= link_to post.title, post_path(post) %></h2>
      <p>
        <%= post.body %>
      </p>
    <% end %>
  </div>
  <div id="sidebar" class="four columns panel right" style="margin-left:45px;">
    <h3>Sidebar</h3>
  </div>
</div>